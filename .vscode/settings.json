{
    "debug.allowBreakpointsEverywhere": true,
    "files.autoGuessEncoding": true,
    "files.exclude": {
        "**/.vscode": true,
        "compile_commands.json": true,
        "*.hrccproj": true,
        "*.sln": true,
        "*.suo": true
